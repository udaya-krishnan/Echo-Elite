        <section class="content-main">
            <div class="content-header">
                <h2 class="content-title">Sellers list</h2>
                
            </div>
            <div class="card mb-4">
                <header class="card-header">
                    <div class="row gx-3">
                        <div class="col-lg-4 col-md-6 me-auto">
                            <input type="text" placeholder="Search..." class="form-control">
                        </div>
                        <div class="col-lg-2 col-md-3 col-6">
                            <select class="form-select">
                                <option>Status</option>
                                <option>Active</option>
                                <option>Disabled</option>
                                <option>Show all</option>
                            </select>
                        </div>
                        <div class="col-lg-2 col-md-3 col-6">
                            <select class="form-select">
                                <option>Show 20</option>
                                <option>Show 30</option>
                                <option>Show 40</option>
                            </select>
                        </div>
                    </div>
                </header> 
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-hover">
                            <thead>
                                <tr>
                                    <th>Seller</th>
                                    <th>Email</th>
                                    <th>Mobile</th>
                                    <th>Status</th>
                                    <th class="text-end"> Action </th>
                                </tr>
                            </thead>
                            <tbody>
                                <% if (userData.length>0) { %>
                                    <% for(let i=0;i<userData.length;i++){ %>
                                            <tr>                                
                                    <td width="40%">
                                        <a href="#" class="itemside">
                                            <div class="info pl-3">
                                                <h6 class="mb-0 title"><%= userData[i].name %></h6>
                                               
                                            </div>
                                        </a>
                                    </td>
                                    <td><%= userData[i].email %></td>
                                    <td><%= userData[i].mobile %></td>
                                 
                                    
                                      
                                        <% if(userData[i].is_blocked == false) { %>
                                            <td><span class="badge rounded-pill alert-success">Unblocked</span></td>

                                            <td class="text-end">
                                            <a href="/admin/block-user?id=<%= userData[i]._id %>" class="btn btn-sm  btn-danger rounded font-sm mt-15">BLOCK</a>
                                            </td>
                                          <% } else { %>
                                            <td><span class="badge rounded-pill alert-danger">Blocked</span></td>
                                            <td class="text-end">
                                            <a href="/admin/unblock-user?id=<%= userData[i]._id %>" class="btn btn-sm btn-success rounded font-sm mt-15">UNBLOCK</a>
                                            </td>
                                          <% } %>
                                          
                                    
                                </tr>
                                    <%}%>
                               <% } %>
                              
                            
                                
                            </tbody>
                        </table> 
                    </div>
                </div> 
            </div> 
