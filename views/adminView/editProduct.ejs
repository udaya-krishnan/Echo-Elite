        <section class="content-main">
            <form action="" method="post" enctype="multipart/form-data">
                <div class="row">
                    <div class="col-lg-9" style="width: 60%;">
                        <div class="content-header">
                            <h2 class="content-title">Add New Product</h2>
                        </div>
                        <div class="card mb-4">
                            <div class="card-header">
                                <h4>Basic</h4>
                            </div>
                            <div class="card-body">
                                <div class="mb-4">
                                    <input type="hidden" id="id" name="id" value="<%=proData._id%>">
                                    <label for="product_name" class="form-label">Product title</label>
                                    <input type="text" placeholder="Type here" class="form-control" id="product_name" name="product_name" value="<%= proData.name %>">
                                </div>
                                <div class="mb-4">
                                    <label class="form-label">Full description</label>
                                    <textarea placeholder="Type here" class="form-control" name="product_dis" rows="4" ><%= proData.discripiton %></textarea>
                                </div>
                                <div class="row">
                                    <div class="col-lg-4">
                                        <div class="mb-4">
                                            <label class="form-label">Regular price</label>
                                            <input placeholder="$" type="text" class="form-control" name="regprice" value="<%= proData.regularPrice %>">
                                        </div>
                                    </div>
                                    <div class="col-lg-4">
                                        <div class="mb-4">
                                            <label class="form-label">Offer price</label>
                                            <input placeholder="$" type="text" class="form-control" name="offprice" value="<%= proData.offerPrice %>">
                                        </div>
                                    </div>
                                    <div class="col-lg-4">
                                        <div class="mb-4">
                                            <label class="form-label">Stock</label>
                                            <input placeholder="" type="text" class="form-control" name="stock" value="<%= proData.stock %>">
                                        </div>
                                    </div>
                                </div>
                                <div class="mb-4">
                                    <label class="form-label">Tax rate</label>
                                    <input type="text" placeholder="%" class="form-control" id="product_name" name="off" value="<%= proData.offPercentage %>">
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-3 mt-4">
                        <div class="card mb-4" style="width: 290px;">
                            <div class="card-header">
                                <h4>Media</h4>
                            </div>
                            <div class="card-body">
                                <div class="input-upload">
                                    <img src="/admin_assets/imgs/theme/upload.svg" alt="">
                                    <input class="form-control" name="proImage" type="file" multiple>
                                </div>
                            </div>
                        </div>
                        <div class="card mb-4" style="width: 290px;">
                            <div class="card-header">
                                <h4>Organization</h4>
                            </div>
                            <div class="card-body">
                                <div class="row gx-2">
                                    <div class="col-sm-6 mb-3">
                                        <label class="form-label">Category</label>
                                        <select class="form-select" name="catName">
                                            <% for( let i = 0; i < catData.length; i++ ) { %>
                                            <% if (catData[i].is_blocked===false) { %>
                                            <option > <%= catData[i].name %> </option>
                                            <% } %>
                                            <% } %>
                                        </select>
                                    </div>
                                    <div class="col-sm-6 mb-3">
                                        <label class="form-label">Brand</label>
                                        <select class="form-select" name="brandName">
                                            <% for( let i = 0; i < brandData.length; i++ ) { %>
                                            <% if (brandData[i].is_blocked===false) { %>
                                            <option> <%= brandData[i].name %> </option>
                                            <% } %>
                                            <% } %>
                                        </select>
                                    </div>
                                </div>
                                <div class="mb-4">
                                    <label for="product_name" class="form-label">Color</label>
                                    <input type="text" class="form-control" name="color" value="black">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row justify-content-end">
                    <div class="col-lg-9">
                        <div>
                            <input type="submit" class="btn btn-outline-secondary " value="Cancel">
                            <input type="submit" class="btn btn-primary " value="Add">
                        </div>
                    </div>
                </div>
            </form>
        </section>
