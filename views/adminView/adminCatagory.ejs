        <section class="content-main">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Categories </h2>
                    <p>Add, edit or delete a category</p>
                </div>
                <div>
                    <input type="text" placeholder="Search Categories" class="form-control bg-white">
                </div>
            </div>
            <div class="d-flex justify-content-end pb-20">
                <a href="/admin/addCategory" class="btn btn-sm btn-primary rounded font-sm">Create Categories</a>
            </div>
            <div class="card">
                <div class="card-body">
             
                        <div class="col-md-15">
                            <div class="table-responsive">
                                <table class="table table-hover">
                                    <thead>
                                        <tr style="width: 100%;">
                                       
                                            <th>Name</th>
                                            <th>Description</th>
                                            <th>Status</th>
                                            <th class="text-end">Action</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% if (catData.length>0) { %>
                                            <% for( let i = 0; i < catData.length; i++ ) { %>
                                                
                                                <tr>
                                                    <td><b><%= catData[i].name %></b></td>
                                                    <td><%= catData[i].discription %></td>
                                                    <% if (catData[i].is_blocked==false) { %>
                                                        <td><span class="badge rounded-pill alert-success">Unlist</span></td>
                                                        <td class="text-end">
                                                            
                                                            <a href="/admin/cat-edit?id=<%= catData[i]._id %>" class="btn btn-sm btn-success rounded font-sm mt-6 p-10">Edit</a>
                                                            <button type="button" class="btn btn-danger" onclick="list('<%= catData[i]._id %>')">List</button>

   
                                                        </td>
                                                    <% } else{%>
                                                        <td><span class="badge rounded-pill alert-danger">List</span></td>
                                                        <td class="text-end">
                                                            <button type="button" class="btn btn-danger" onclick="list('<%= catData[i]._id %>')">Unlist</button>
                                                        </td>

                                                        <% } %>
                                                                                             
                                                   
                                                </tr>
                                            
                                            <% } %>
                                            
                                        <% } %>
                                        
                                    </tbody>
                                </table>
                            </div>
                        </div> 
                
                </div> 
            </div>
        </section> 
