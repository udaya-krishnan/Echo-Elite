    <script>
        function list(id){
            // alert(id)
           
            $.ajax({
                url:"/admin/cat-list?id="+id,
                method:"get",
               success:(response)=>{
                if(response.status===true){

                    window.location.href="http://localhost:2003/admin/category"
                }
               }
            })
            
   
        }



       
   </script>


