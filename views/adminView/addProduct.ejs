    <script>

        function nameFun() {
            const name = document.getElementById("product_name").value
            const nameRegex = /^[A-Za-z\s]+$/;
            if (name.trim() == '') {
                document.getElementById("nameErr").innerHTML = "Product name is require"
            } else {
                document.getElementById("nameErr").innerHTML = ""
            }
            if (nameRegex.test(name)) {
                document.getElementById("nameErr").innerHTML = "Invalid Name"
            } else {
                document.getElementById("nameErr").innerHTML = ""
            }
        }

        function disFun() {
            const dis = document.getElementById("product_dis").value
            if (dis.trim() == '') {
                document.getElementById("disErr").innerHTML = "Product discription  is require"
            } else {
                document.getElementById("disErr").innerHTML = ""
            }
        }

        function rpFun() {
            const rp = document.getElementById("regprice").value

            const regex = /^[0-9]+$/

            if (rp.trim() == '') {
                document.getElementById("rpErr").innerHTML = "Regular price is require"
            } else {
                document.getElementById("rpErr").innerHTML = ""
            }
            if (regex.test(rp)) {
                document.getElementById("rpErr").innerHTML = "Invalid price"
            } else {
                document.getElementById("rpErr").innerHTML = ""
            }
        }

        function opFun() {
            const op = document.getElementById("offprice").value

            const regex = /^[0-9]+$/

            if (op.trim() == '') {
                document.getElementById("opErr").innerHTML = "Regular price is require"
            } else {
                document.getElementById("opErr").innerHTML = ""
            }
            if (regex.test(op)) {
                document.getElementById("opErr").innerHTML = "Invalid price"
            } else {
                document.getElementById("opErr").innerHTML = ""
            }
        }


        function stFun() {
            const st = document.getElementById("stock").value

            const regex = /^[0-9]+$/

            if (st.trim() == '') {
                document.getElementById("stErr").innerHTML = "Stock is require"
            } else {
                document.getElementById("stErr").innerHTML = ""
            }
            if (regex.test(st)) {
                document.getElementById("stErr").innerHTML = "Invalid price"
            } else {
                document.getElementById("stErr").innerHTML = ""
            }
        }


        function ofFun() {
            const st = document.getElementById("off").value

            const regex = /^[0-9]+$/

            if (st.trim() == '') {
                document.getElementById("offErr").innerHTML = "Offer Pecentage is require"
            } else {
                document.getElementById("offErr").innerHTML = ""
            }
            if (regex.test(st)) {
                document.getElementById("offErr").innerHTML = "Invalid price"
            } else {
                document.getElementById("offErr").innerHTML = ""
            }
            if(st>100){ 
                document.getElementById("offErr").innerHTML = "Miximum 100%"
            }else {
                document.getElementById("offErr").innerHTML = ""
            }
            if(st<=0){
                document.getElementById("offErr").innerHTML = "Minimum 100%"
            }else {
                document.getElementById("offErr").innerHTML = ""
            }
        }

        function coFun() {
            const col = document.getElementById("color").value
            const nameRegex = /^[A-Za-z\s]+$/;
            if (col.trim() == '') {
                document.getElementById("coErr").innerHTML = "Color is require"
            } else {
                document.getElementById("coErr").innerHTML = ""
            }
            if (nameRegex.test(col)) {
                document.getElementById("coErr").innerHTML = "Invalid Color"
            } else {
                document.getElementById("coErr").innerHTML = ""
            }
        }


        document.getElementById("submitId").addEventListener("submit",(e)=>{
            nameFun()
            disFun()
            rpFun()
            coFun()
            ofFun()
            stFun()
            opFun()

            const name=document.getElementById("nameErr")
            const dis=document.getElementById("disErr")
            const gp=document.getElementById("rpErr")
            const op=document.getElementById("opErr")
            const st=document.getElementById("stErr")
            const of=document.getElementById("offErr")
            const co=document.getElementById("coErr")

            if(name.innerHTML||dis.innerHTML||gp.innerHTML||op.innerHTML||st.innerHTML||of.innerHTML||co.innerHTML){
                e.preventDefault()
            }
            
        })


        function cancel(){
            const name = document.getElementById("product_name").value.trim()
            const dis = document.getElementById("product_dis").value.trim()
            const rp = document.getElementById("regprice").value.trim()
            const op = document.getElementById("offprice").value.trim()
            const st = document.getElementById("stock").value.trim()
            const sf = document.getElementById("off").value.trim()
            const col = document.getElementById("color").value.trim()

            if(name==''&&dis==''&&rp==''&&op==''&&st==''&&sf==''&&col==''){
                window.location.href = "http://localhost:2003/admin/product"
            }else{
                //  alert("h")
                 Swal.fire({
                    title: "Are you sure?",
                    text: "You won't be able to revert this!",
                    icon: "warning",
                    showCancelButton: true,
                    confirmButtonColor: "#d33",
                    cancelButtonColor: "#3085d6",
                    confirmButtonText: "Discard", // Swapping the buttons
                    cancelButtonText: "Cancel"     // Swapping the buttons
                }).then((result) => {
                    if (result.isConfirmed) {
                        Swal.fire({
                            title: "Cancel!",
                            text: "Your filed has been removed.",
                            icon: "success"
                        }).then(()=>{
                            window.location.href = "http://localhost:2003/admin/product"
                        })
                    }
                })
            }
        }





    </script>




