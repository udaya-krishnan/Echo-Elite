  <script>
    function Block(id,index){
        // alert(id)
      $.ajax({
        url:"/admin/coupon-block",
        method:"post",
        data:{
            id
        },
        success:(response)=>{
            if(response.status==true){
                // alert("hello")
                document.getElementById("edit"+index).style.display="none"
                    document.getElementById("listButton"+index).innerHTML='Active'
                    document.getElementById('status'+index).innerHTML='Not Active'
            }else if(response.status==false){
               
                    document.getElementById("edit"+index).style.display="inline-block"
                    document.getElementById("listButton"+index).innerHTML='Not Active'
                    document.getElementById('status'+index).innerHTML='Active'
            }
        }
      })
    }
  </script>