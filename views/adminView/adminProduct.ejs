                <div>
                    <a href="#" class="btn btn-light rounded font-md">Export</a>
                    <a href="#" class="btn btn-light rounded  font-md">Import</a>
                    <a href="/admin/addProduct" class="btn btn-primary btn-sm rounded">Create new</a>
                </div>
            </div>
            <div class="card mb-4">
                <header class="card-header">
                    <div class="row gx-3">
                        <div class="col-lg-4 col-md-6 me-auto">
                            <input type="text" placeholder="Search..." class="form-control">
                        </div>
                        <div class="col-lg-2 col-6 col-md-3">
                            <select class="form-select">
                                <option>All category</option>
                                <option>Electronics</option>
                                <option>Clothings</option>
                                <option>Something else</option>
                            </select>
                        </div>
                        <div class="col-lg-2 col-6 col-md-3">
                            <select class="form-select">
                                <option>Latest added</option>
                                <option>Cheap first</option>
                                <option>Most viewed</option>
                            </select>
                        </div>
                    </div>
                </header> <!-- card-header end// -->
                <div class="card-body">
                    <div class="row gx-3 row-cols-1 row-cols-sm-2 row-cols-md-3 row-cols-xl-4 row-cols-xxl-5">

                        <% for( let i = 0; i< proData.length; i++ ) { %>
                        
                            <div class="col">
                                <div class="card card-product-grid">
                                    <a href="#" class="img-wrap"> <img style="width: 200px; height: 250px;" src="/productImages/<%= proData[i].image[4] %>" alt="Product" > </a>
                                    <div class="info-wrap">
                                        <a href="#" class="title text-truncate"><%= proData[i].name %></a>
                                        <div class="price mb-2">$<%= proData[i].offerPrice %></div> 
                                        <% if (proData[i].is_blocked==true ) { %>
                                            <a href="/admin/block-pro?id=<%= proData[i]._id %>" class="btn btn-sm font-sm btn-light rounded">
                                                <i class="material-icons md-delete_forever"></i> unlist
                                            </a>
                                      
                                        <% } else { %>
                                         
                                        <a href="/admin/edit-pro?id=<%= proData[i]._id %>" class="btn btn-sm font-sm rounded btn-brand">
                                            <i class="material-icons md-edit"></i> Edit
                                        </a>
                                        <a href="/admin/block-pro?id=<%= proData[i]._id %>" class="btn btn-sm font-sm btn-light rounded">
                                            <i class="material-icons md-delete_forever"></i> list
                                        </a>
                                       <% } %> 
                                    </div>
                                </div> 
                            </div> 
                        
                        <% } %>
                        
                               
            </div> <!-- card end// -->
