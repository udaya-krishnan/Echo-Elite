<script>
    function addCart(index) {
        const id = document.getElementById("proId"+index).value
        const proPrice = document.getElementById("offerPrice").innerHTML



        // alert(proPrice)



          alert(id)

        $.ajax({
            url: "/addCatLoad",
            method: "post",
            data: {
                id,
                proPrice
            },
            success: (response) => {
                if (response.status == true) {
                    window.location.href = "http://localhost:2003/cart"
                } else if (response.status == "viewCart") {
                    window.location.href = "http://localhost:2003/cart"
                }
            }

        })

    }
</script>

