<script>

    function addCart() {


        const id = document.getElementById("proId").value
        const proPrice = document.getElementById("offerPrice").innerHTML





        $.ajax({
            url: "/addCatLoad",
            method: "post",
            data: {
                id,
                proPrice
            },
            success: (response) => {
                if (response.status == true) {
                    Swal.fire({

                        icon: "success",
                        title: "Add to Cart",
                        showConfirmButton: false,
                        timer: 1500
                    }).then((result) => {
                        window.location.href = "http://localhost:2003/cart"
                    })

                } else if (response.status == "viewCart") {
                    window.location.href = "http://localhost:2003/cart"
                } else if (response.status == "login") {

                    Swal.fire({

                        text: "You want to login the page?!",
                        icon: "warning",
                        showCancelButton: true,
                        confirmButtonColor: "#3085d6",
                        cancelButtonColor: "#d33",
                        confirmButtonText: "Go to login!"
                    }).then((result) => {
                        if (result.isConfirmed) {
                            window.location.href = "/login"
                        }
                    })
                }
            }

        })



    }

    function outOfStock() {
        Swal.fire({
            title: "Out of Stock?",
            icon: "warning"
        });
    }


    function viewCart() {
        location.href = "/cart"
    }



    function addWishList(id) {
        // alert(id)
        $.ajax({
            url: '/addWishlist',
            method: 'post',
            data: {
                id
            },
            success: (response) => {
                if (response.status == true) {

                    const Toast = Swal.mixin({
                        toast: true,
                        position: "bottom",
                        showConfirmButton: false,
                        timer: 1500,
                        // timerProgressBar: true,
                        didOpen: (toast) => {
                            toast.onmouseenter = Swal.stopTimer;
                            toast.onmouseleave = Swal.resumeTimer;
                        }
                    });
                    Toast.fire({
                        icon: "success",
                        title: "Add to wishlist"
                    }).then((result) => {
                        document.getElementById("addWishlist").style.display = "none"
                        document.getElementById("removeWish").style.display = "inline-block"
                        //    alert('hello')
                    })
                } else if (response.status == "login") {
                    Swal.fire({

                        text: "You want to login the page?!",
                        icon: "warning",
                        showCancelButton: true,
                        confirmButtonColor: "#3085d6",
                        cancelButtonColor: "#d33",
                        confirmButtonText: "Go to login!"
                    }).then((result) => {
                        if (result.isConfirmed) {
                            window.location.href = "/login"
                        }
                    })
                }
            }
        })
    }

    function removeWishlist(id) {

        $.ajax({
            url: "/removeWish",
            method: "post",
            data: {
                id
            },
            success: (response) => {
                if (response.status == true) {

                    const Toast = Swal.mixin({
                        toast: true,
                        position: "bottom",
                        showConfirmButton: false,
                        timer: 1500,

                        // timerProgressBar: true,
                        didOpen: (toast) => {
                            toast.onmouseenter = Swal.stopTimer;
                            toast.onmouseleave = Swal.resumeTimer;
                        }
                    });
                    Toast.fire({
                        icon: "success",
                        title: "Remove from wishlist"
                    }).then((result) => {
                        document.getElementById("addWishlist").style.display = "inline-block"
                        document.getElementById("removeWish").style.display = "none"
                        //    alert('hello')
                    })
                }
            }
        })
    }


    const stars = document.querySelectorAll('.custom-star-rating input[type="radio"]');
    // alert(stars)
    let rating;
    stars.forEach(star => {
        star.addEventListener('click', () => {
            rating = star.value;

            if (rating == 5) {
                document.getElementById("5-star").style.color = "#ffc107"
                document.getElementById("4-star").style.color = "#ffc107"
                document.getElementById("3-star").style.color = "#ffc107"
                document.getElementById("2-star").style.color = "#ffc107"
                document.getElementById("1-star").style.color = "#ffc107"
            } else if (rating == 4) {
                document.getElementById("5-star").style.color = "#4f5d77"
                document.getElementById("4-star").style.color = "#ffc107"
                document.getElementById("3-star").style.color = "#ffc107"
                document.getElementById("2-star").style.color = "#ffc107"
                document.getElementById("1-star").style.color = "#ffc107"
            } else if (rating == 3) {
                document.getElementById("5-star").style.color = "#4f5d77"
                document.getElementById("4-star").style.color = "#4f5d77"
                document.getElementById("3-star").style.color = "#ffc107"
                document.getElementById("2-star").style.color = "#ffc107"
                document.getElementById("1-star").style.color = "#ffc107"
            } else if (rating == 2) {
                document.getElementById("5-star").style.color = "#4f5d77"
                document.getElementById("4-star").style.color = "#4f5d77"
                document.getElementById("3-star").style.color = "#4f5d77"
                document.getElementById("2-star").style.color = "#ffc107"
                document.getElementById("1-star").style.color = "#ffc107"
            } else if (rating == 1) {
                document.getElementById("5-star").style.color = "#4f5d77"
                document.getElementById("4-star").style.color = "#4f5d77"
                document.getElementById("3-star").style.color = "#4f5d77"
                document.getElementById("2-star").style.color = "#4f5d77"
                document.getElementById("1-star").style.color = "#ffc107"
            }
        });
    });

    function disfun() {
        const dis = document.getElementById("comment").value

        if (dis == "") {
            document.getElementById("disErr").innerHTML = "Please write your response"
        } else {
            document.getElementById("disErr").innerHTML = ""
        }
    }

    function namefun() {
        const name = document.getElementById('name').value
        if (name == "") {
            document.getElementById("nameErr").innerHTML = "please mention your name"
        } else {
            document.getElementById("nameErr").innerHTML = ""
        }
    }


    function emailfun() {
        const email = document.getElementById("email").value
        const emailRegex = /^[^\s@]+@[^\s@]+\.[^\s@]+$/;
        if (email == "") {
            document.getElementById("emailErr").innerHTML = "please Enter your Email"
        } else {
            document.getElementById("emailErr").innerHTML = ""
        }
        if (!emailRegex.test(email)) {
            document.getElementById("emailErr").innerHTML = "invalid format"
        } else {
            document.getElementById("emailErr").innerHTML = ""
        }
    }





    function review(id) {
        // alert(rating)
        namefun()
        disfun()
        emailfun()

        const nameErr = document.getElementById("nameErr")
        const emailErr = document.getElementById("emailErr")
        const disErr = document.getElementById("disErr")


        if (nameErr.innerHTML || emailErr.innerHTML || disErr.innerHTML) {
            // alert("hai")
            document.getElementById("submitErr").innerHTML = "please fill the correct"

            setTimeout(() => {
                document.getElementById("submitErr").innerHTML = ""
            }, 2000)
        } else if (rating == undefined) {
            // alert("hello")
            document.getElementById("starErr").innerHTML = "please put your Rating"

            setTimeout(() => {
                document.getElementById("starErr").innerHTML = ""
            }, 2000)
        } else {
            // alert(id)
            const dis = document.getElementById("comment").value
            // alert(dis)
            const name = document.getElementById('name').value
            // alert(name)
            const email = document.getElementById("email").value
            // alert(email)
            // alert(rating)


            $.ajax({
                url: "/rating",
                method: "post",
                data: {
                    name,
                    dis,
                    email,
                    rating,
                    id
                },
                success: (response) => {
                    if (response.status == true) {
                        alert("helloo")
                        const Toast = Swal.mixin({
                            toast: true,
                            position: "bottom",
                            showConfirmButton: false,
                            timer: 1500,

                            // timerProgressBar: true,
                            didOpen: (toast) => {
                                toast.onmouseenter = Swal.stopTimer;
                                toast.onmouseleave = Swal.resumeTimer;
                            }
                        });
                        Toast.fire({
                            icon: "success",
                            title: "Your Response Sended"
                        }).then((result) => {
                            window.location.href = `/showproduct?id=${response.id}`
                        })

                    }
                }
            })
        }

    }

    const inputElement = document.getElementById('input');
    let timeoutId;

    function debounce(func, delay) {
        return function () {
            clearTimeout(timeoutId);
            timeoutId = setTimeout(() => {
                func();
            }, delay);
        };
    }

    function showSearchResult() {
        const searchResult = document.querySelector('.search-result');
        searchResult.style.display = 'block';
    }

    function hideSearchResult() {
        const searchResult = document.querySelector('.search-result');
        searchResult.style.display = 'none';
    }

    function showNoProductFound() {
        const noProductFound = document.querySelector('.no-product-found');
        noProductFound.style.display = 'block';
    }

    function hideNoProductFound() {
        const noProductFound = document.querySelector('.no-product-found');
        noProductFound.style.display = 'none';
    }

    function SearchProduct() {
        const searchDataValue = inputElement.value.trim();


        if (searchDataValue !== "") {
            showSearchResult();
            debounce(() => {
                console.log(searchDataValue)
                $.ajax({
                    url: "/search",
                    method: "post",
                    data: {
                        searchDataValue
                    },
                    success: (response) => {
                        const searchResultsContainer = document.getElementById('searchResultsContainer')
                        searchResultsContainer.innerHTML = ""

                        if (response.searchProducts.length > 0) {
                            response.searchProducts.forEach(element => {
                                const productContainer = document.createElement('div');
                                productContainer.classList.add('product-container');

                                const productElement = document.createElement('div');
                                productElement.innerHTML = `
                                <a href="/showproduct?id=${element._id}" class="product-link">
                                    <img src="/productImages/${element.image[0]}" alt="product">
                                    <div class="product-info">
                                        <p>${element.name}</p>
                                        <p>₹${element.regularPrice}</p>
                                    </div>
                                `;

                                productContainer.appendChild(productElement);
                                searchResultsContainer.appendChild(productContainer);
                            });
                            hideNoProductFound();
                        } else {
                            hideSearchResult();
                            showNoProductFound();
                        }
                    }
                })
            }, 500)();
        } else {
            hideSearchResult();
            hideNoProductFound();
        }
    }


</script>

